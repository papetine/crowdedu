  {% for label, messages in app.flashes(['warning', 'error', 'success']) %}
        {% for message in messages %}
          <div class="alert alert-{{ label }}">
            {{ message }}
          </div>
        {% endfor %}
      {% endfor %}
  </div>
      <div class="col-lg-6 col-md-4 col-sm-6 col-xs-6 col-xxs-12">
      <div class="card">
        <div class="card-header "style="background-color:#96bf31;color:#ffffff;font-size:20px;font-family:ubuntu;text-align:center;">
         <span ><b style="margin-top:50px">S'inscrire</b></span>
        </div>
        <div class="card-body">
           {{form_start(form)}}
                <div class="form-row ">
                <div class="form-group col-md-6">       
                  {{form_row(form.nom,{'attr':{'class':'form-control'}}) }}
                </div>
               <div class="form-group col-md-6">         
                {{form_row(form.prenom,{'attr':{'class':'form-control'}}) }}
              </div>
              </div>
               <div class="form-row ">
                <div class="form-group col-md-6">          
                {{form_row(form.username,{'attr':{'class':'form-control'}}) }}
                </div>
              <div class="form-group col-md-6">          
                {{form_row (form.email,{'attr':{'class':'form-control'}}) }}
                </div>
                </div>
                    <div class="form-row ">
                <div class="form-group col-md-6">  
                    {{form_row(form.password.first,{'attr':{'class':'form-control'}}) }}
                  </div>
                  <div class="form-group col-md-6">  
                  {{form_row(form.password.second,{'attr':{'class':'form-control'}}) }} 
                </div>  
                </div>
               <div class="form-row ">
                <div class="form-group col-md-6">  
                  {{form_row(form.dateNaissance, {'attr':{'class':'form-control'}}) }} 
                </div> 
                  <div class="form-group col-md-6">  
                  {{form_row(form.lieuNaissance,{'attr':{'class':'form-control'}}) }} 
                </div> 
                </div>
                <div class="form-check-inline"> 
                  {{form_row(form.sexe,{'attr':{'class':'form-check-input'}}) }} 
                </div>
                
        </div>
        <div class="card-footer text-muted">
        <div class="row">
                  <div class="col-lg-4"></div>
                    <div class="col-lg-4">
                    <h4 class="card-title"><button class="btn" type="submit"style="background-color:#96bf31;color:#ffffff;font-family:ubuntu;">S'inscrire</button></h4>
                    </div>
                    <div class="col-lg-4"></div>
                  </div>
        </div>
